    <link rel="stylesheet" href="/css/mod.water.css">
    <link rel="stylesheet" href="/css/extra.css">
    <script src="/js/jquery.js"></script>
    <script src="/js/navbar.js"></script>
    <script src="/js/custom_calc.js"></script>
    <script data-ad-client="ca-pub-3206752623625887" async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
