<body>
    <div class="main pre valign mobile-center">
        <h3>Custom plan</h3>

        <table class="otable">
            <tbody>
                <tr>
                    <th></th>
                    <th></th>
                    <th></th>
                </tr>
                <tr>
                    <td>Storage:</td>
                    <td><input type="range" min="1" max="200" value="100" class="slider eighty-width" id="_Storage"></td>
                    <td><span id="_stcounter">100</span>GB</td>
                </tr>
                <tr>
                    <td>Memory:</td>
                    <td><input type="range" min="0.1" max="3" value="1.5" step="0.1" class="slider eighty-width" id="_MemoryG"></td>
                    <td><span id="_mmcounter">1.5</span>GB</td>
                </tr>
                <tr>
                    <td>Bandwidth:</td>
                    <td><input type="range" min="1" max="25" value="12.5" class="slider eighty-width" id="_Bndwdth"></td>
                    <td><span id="_bwcounter">12.5</span>MB</td>
                </tr>
                <tr>
                    <td>CPU:</td>
                    <td><input type="range" min="1" max="3"   value="2" class="slider eighty-width" id="_CPUCore"></td>
                    <td><span id="_cccounter">2</span> Cores</td>
                </tr>
                <tr>
                    <td>Total:</td>
                    <td></td>
                    <td>$<span id="_mncounter"></span></td>
                </tr>
            </tbody>
        </table>
        <a id="_mtbutton" href=""><button>Request</button></a></div>
    </div>
